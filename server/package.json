{
  "name": "apokrupto-server",
  "version": "1.0.0",
  "description": "Apokrupto - Node.js backend server",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "docker:start": "docker-compose up --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.helper\\.js$"
    ]
  },
  "dependencies": {
    "@turf/turf": "^7.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.0.0",
    "pg": "^8.16.3",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^30.2.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.8.1"
  },
  "private": true,
  "keywords": [],
  "author": "",
  "license": "ISC"
}
