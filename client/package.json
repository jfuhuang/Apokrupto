{
  "name": "apokrupto-client",
  "version": "1.0.0",
  "description": "Apokrupto - React Native mobile client",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:prod": "EXPO_PUBLIC_API_URL=https://apokrupto-server.onrender.com expo start",
    "android": "expo run:android",
    "android:prod": "EXPO_PUBLIC_API_URL=https://apokrupto-server.onrender.com expo run:android",
    "ios": "expo run:ios",
    "ios:prod": "EXPO_PUBLIC_API_URL=https://apokrupto-server.onrender.com expo run:ios",
    "web": "expo start --web",
    "web:prod": "EXPO_PUBLIC_API_URL=https://apokrupto-server.onrender.com expo start --web",
    "tunnel": "expo start --tunnel",
    "tunnel:prod": "EXPO_PUBLIC_API_URL=https://apokrupto-server.onrender.com expo start --tunnel",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@expo-google-fonts/exo-2": "^0.4.2",
    "@expo-google-fonts/orbitron": "^0.4.2",
    "@expo-google-fonts/rajdhani": "^0.4.1",
    "expo": "^54.0.32",
    "expo-av": "~16.0.8",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-screen-orientation": "~9.0.8",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^8.0.1",
    "eslint-config-prettier": "^9.1.2",
    "prettier": "^3.8.1"
  },
  "private": true
}
